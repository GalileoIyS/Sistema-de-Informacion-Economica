                        <div id="PanelSignUp" class="hidden">
                            <asp:UpdatePanel ID="UpdatePanel1" runat="server" ChildrenAsTriggers="true">
                                <ContentTemplate>
                                    <asp:CreateUserWizard ID="RegistroDeUsuarioForm" OnCreateUserError="RegistroDeUsuarioForm_CreateUserError" runat="server" ActiveStepIndex="0" DisableCreatedUser="True" CreateUserButtonText="Create Account" LoginCreatedUser="true" ContinueDestinationPageUrl="~/Default.aspx" OnCreatedUser="RegistroDeUsuarioForm_CreatedUser">
                                        <CreateUserButtonStyle CssClass="btn btn-dropkeys" />
                                        <WizardSteps>
                                            <asp:CreateUserWizardStep ID="CreateUserWizardStep1" runat="server">
                                                <ContentTemplate>
                                                    <fieldset>
                                                        <div class="form-group col-xs-12 col-lg-12">
                                                            <!-- EMAIL -->
                                                            <asp:Label ID="EmailLabel" runat="server" AssociatedControlID="Email"><i class="fa fa-envelope-o"></i>&nbsp;E-mail</asp:Label>
                                                            <asp:RequiredFieldValidator ID="EmailRequired" runat="server" ControlToValidate="Email" ValidationGroup="RegistroDeUsuarioForm" CssClass="label bg-color-blue" ForeColor="#ffffff"><span class="glyphicon glyphicon-warning-sign"></span>&nbsp;E-mail is required</asp:RequiredFieldValidator>
                                                            <asp:RegularExpressionValidator ID="EmailRegularExpressionValidator" runat="server" Display="Dynamic" ValidationExpression="\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*"
                                                                ControlToValidate="Email" ValidationGroup="RegistroDeUsuarioForm" CssClass="label bg-color-blue" ForeColor="#ffffff"><span class="glyphicon glyphicon-warning-sign"></span>&nbsp;The format of e-mail is invalid</asp:RegularExpressionValidator>
                                                            <asp:TextBox ID="Email" runat="server" placeholder="Enter your e-mail address" CssClass="form-control" ValidationGroup="RegistroDeUsuarioForm" MaxLength="255"></asp:TextBox>
                                                        </div>
                                                        <div class="form-group col-xs-12 col-lg-12">
                                                            <!-- PASSWORD -->
                                                            <asp:Label ID="PasswordLabel" runat="server" AssociatedControlID="Password"><i class="fa fa-key"></i>&nbsp;Password</asp:Label>
                                                            <asp:RequiredFieldValidator ID="PasswordRequired" runat="server" ControlToValidate="Password" ValidationGroup="RegistroDeUsuarioForm" CssClass="label bg-color-blue" ForeColor="#ffffff"><span class="glyphicon glyphicon-warning-sign"></span>&nbsp;Password is required</asp:RequiredFieldValidator>
                                                            <asp:TextBox ID="Password" runat="server" TextMode="Password" placeholder="Enter your password" ValidationGroup="RegistroDeUsuarioForm" CssClass="form-control" MaxLength="30"></asp:TextBox>

                                                        </div>
                                                        <div class="form-group col-xs-12 col-lg-12">
                                                            <!-- CONFIRM PASSWORD -->
                                                            <asp:Label ID="ConfirmPasswordLabel" runat="server" AssociatedControlID="ConfirmPassword"><i class="fa fa-key"></i>&nbsp;Confirm password</asp:Label>
                                                            <asp:RequiredFieldValidator ID="ConfirmPasswordRequired" runat="server" ControlToValidate="ConfirmPassword" ValidationGroup="RegistroDeUsuarioForm" CssClass="label bg-color-blue" ForeColor="#ffffff"><span class="glyphicon glyphicon-warning-sign"></span>&nbsp;Please, retype the password</asp:RequiredFieldValidator>
                                                            <asp:CompareValidator ID="PasswordCompare" runat="server" ControlToCompare="Password" ControlToValidate="ConfirmPassword" Display="Dynamic" ValidationGroup="RegistroDeUsuarioForm" CssClass="label label-danger" ForeColor="#ffffff"><span class="glyphicon glyphicon-warning-sign"></span>&nbsp;Passwords do not match</asp:CompareValidator>
                                                            <asp:TextBox ID="ConfirmPassword" runat="server" TextMode="Password" placeholder="Don't forget your password" ValidationGroup="RegistroDeUsuarioForm" CssClass="form-control" MaxLength="30"></asp:TextBox>
                                                        </div>
                                                        <hr></hr>
                                                        <div class="form-group col-lg-6">
                                                            <asp:Label ID="lbFirstName" runat="server" AssociatedControlID="txtFirstName"><i class="fa fa-user"></i>&nbsp;First name</asp:Label>
