                                                </div>
                                                <div class="form-group col-xs-12 col-lg-12">
                                                    <asp:LinkButton runat="server" ID="toogleToRecovery" class="small pull-right" ClientIDMode="Static" OnClick="btnRecovery_Click">Forgot password?</asp:LinkButton>
                                                    <asp:Label ID="PasswordLabel" runat="server" AssociatedControlID="Password"><i class="fa fa-key"></i>&nbsp;Password</asp:Label>
                                                    <asp:RequiredFieldValidator ID="RequiredFieldPassword" runat="server" ValidationGroup="LoginUserValidation" ControlToValidate="Password" CssClass="label bg-color-blue" ForeColor="#ffffff"><span class="glyphicon glyphicon-warning-sign"></span>&nbsp;Password is required</asp:RequiredFieldValidator>
                                                    <asp:TextBox ID="Password" runat="server" TextMode="Password" placeholder="Enter your password"
                                                        TabIndex="2" autocomplete="off" CssClass="form-control" ValidationGroup="LoginUserValidation"></asp:TextBox>
                                                </div>
                                                <asp:Literal ID="FailureText" runat="server" EnableViewState="False"></asp:Literal>
                                                <div class="form-group col-xs-12 col-lg-12">
                                                    <asp:Button ID="LoginButton" runat="server" CommandName="Login" Text="Login" ValidationGroup="LoginUserValidation"
                                                        CausesValidation="true" TabIndex="2" CssClass="btn btn-dropkeys pull-right" />
                                                </div>
                                            </fieldset>
                                            <asp:Panel runat="server" ID="PanelRecoveryPassword" ClientIDMode="Static" EnableViewState="true">
                                                <fieldset>
                                                    <div class="form-group col-xs-12 col-lg-12 has-success">
                                                        <asp:Label runat="server" ID="RecoveryPasswordLabel" AssociatedControlID="txtPasswordRecovery"><i class="fa fa-keyboard-o"></i>&nbsp;Enter the e-mail address and we will send you another new one</asp:Label>
                                                        <asp:RequiredFieldValidator ID="RequiredFieldValidator1" runat="server" ValidationGroup="LoginUserRecovery"
                                                            ControlToValidate="txtPasswordRecovery">*</asp:RequiredFieldValidator>
                                                        <asp:TextBox ID="txtPasswordRecovery" runat="server" placeholder="Introduce your e-mail"
                                                            ValidationGroup="LoginUserRecovery" TabIndex="1" CssClass="form-control required email" ClientIDMode="Static"></asp:TextBox>
                                                    </div>
                                                    <div class="form-group col-xs-12 col-lg-12">
