                                        <LayoutTemplate>
                                            <fieldset>
                                                <div class="form-group col-xs-12 col-lg-12">
                                                    <asp:Label runat="server" ID="UserNameLabel" AssociatedControlID="UserName"><i class="fa fa-envelope-o"></i>&nbsp;E-mail</asp:Label>
                                                    <asp:RequiredFieldValidator ID="RequiredFieldUserName" runat="server" ValidationGroup="LoginUserValidation" ControlToValidate="UserName" CssClass="label bg-color-blue" ForeColor="#ffffff"><span class="glyphicon glyphicon-warning-sign"></span>&nbsp;E-mail is required</asp:RequiredFieldValidator>
