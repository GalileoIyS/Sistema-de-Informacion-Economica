                                                            <asp:TextBox ID="txtFirstName" runat="server" placeholder="Enter your first name" ValidationGroup="RegistroDeUsuarioForm" CssClass="form-control" MaxLength="40"></asp:TextBox>
                                                        </div>
                                                        <div class="form-group col-lg-6">
